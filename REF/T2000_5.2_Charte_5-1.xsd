	<xsd:element name="NCA_Surnav" type="NCA_SurnavType"/>
	<xsd:element name="NCA_Vessel_List" type="NCA_VesselListType"/>
	<xsd:element name="NCA_Voyage" type="NCA_VoyageType"/>
	<xsd:element name="NCA_Voyage_Req" type="NCA_Voyage_ReqType"/>
	<xsd:element name="NCA_Voyage_Res" type="NCA_VoyageResType"/>
