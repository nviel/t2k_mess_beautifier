	<xsd:complexType name="NCA_HeaderType">
		<xsd:attribute name="Version" type="xsd:string" use="required" fixed="5.2"/>
		<xsd:attribute name="TestId" type="TestIdType" use="optional"/>
		<xsd:attribute name="LARefId" type="RefIdType" use="required"/>
		<xsd:attribute name="SentAt" type="zsdateTime" use="required"/>
		<xsd:attribute name="From" type="FromType" use="required"/>
		<xsd:attribute name="To" type="ToType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="VoyageInformationNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="PortOfCall" type="LocodeType" use="required"/>
		<xsd:attribute name="NextPortOfCall" type="LocodeType" use="required"/>
		<xsd:attribute name="ETA" type="zsdateTime"/>
		<xsd:attribute name="TotalPassenger" type="xsd:integer"/>
		<xsd:attribute name="CrewNumber" type="xsd:integer"/>
		<xsd:attribute name="TotalClandestine" type="xsd:integer"/>
		<xsd:attribute name="ReportingDateAndTime" type="zsdateTime" use="required"/>
		<xsd:attribute name="COG" type="COGType" use="required"/>
		<xsd:attribute name="SOG" type="SOGType" use="required"/>
		<xsd:attribute name="NavigationStatus" type="xsd:string" use="required"/>
		<xsd:attribute name="Draught" type="DraughtType" use="required"/>
		<xsd:attribute name="Lane" type="xsd:string" use="required"/>
		<xsd:attribute name="TrackNumber" type="xsd:integer"/>
		<xsd:attribute name="Observations" type="ZeroToTwoHundredFiftyFiveCharType"/>
		<xsd:attribute name="Towing" type="TowingType"/>
		<xsd:attribute name="TowLength" type="xsd:integer"/>
		<xsd:attribute name="TowCategory" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="DGNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DGDetails" type="DGDetailsNCAType" minOccurs="0" maxOccurs="99"/>
		</xsd:sequence>
		<xsd:attribute name="AnyDG" type="BooleanType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="NotificationDetailsType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="CargoManifestNCAType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="CargoManifestNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="URLDetails" type="URLDetailsNCAType" minOccurs="0"/>
			<xsd:element name="ContactDetails" type="ContactDetailsNCAType" minOccurs="0"/>
		</xsd:choice>
	</xsd:group>
	<xsd:complexType name="ContactDetailsNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="LastName" type="ZeroToFiftyCharType"/>
		<xsd:attribute name="FirstName" type="ZeroToFiftyCharType"/>
		<xsd:attribute name="LoCode" type="LocodeType"/>
		<xsd:attribute name="Phone" type="OneToTwentyCharType"/>
		<xsd:attribute name="Fax" type="OneToTwentyCharType"/>
		<xsd:attribute name="EMail" type="ZeroToFiftyCharType"/>
	</xsd:complexType>
	<xsd:complexType name="CargoOfInterestNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="CargoOfInterestCategory" type="OneToTwentyCharType" use="required"/>
		<xsd:attribute name="Quantity" type="xsd:string" use="required"/>
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="CargoInformationNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="TotalQuantity" type="xsd:integer"/>
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string"/>
		<xsd:attribute name="Ballast" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="CargoInformationNCAAnchorSurnavType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CargoInformationNCAType">
				<xsd:sequence>
					<xsd:element name="DG" type="DGNCAType"/>
					<xsd:element name="CargoManifest" type="NotificationDetailsType" minOccurs="0"/>
					<xsd:element name="CargoOfInterest" type="CargoOfInterestNCAType" minOccurs="0" maxOccurs="99"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DGNCAShipInfType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DGDetails" type="DGDetailsNCAType" minOccurs="0" maxOccurs="99"/>
		</xsd:sequence>
		<xsd:attribute name="AnyDG" type="BooleanType"/>
	</xsd:complexType>
	<xsd:complexType name="SearchCriteriaNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="IMONumber" type="IMONumberType"/>
		<xsd:attribute name="MMSINumber" type="MMSINumberType"/>
	</xsd:complexType>
	<xsd:complexType name="DiffusionDestinataireNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="Name" type="OneToFiftyCharType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="BunkersType">
		<xsd:attribute name="Chars" type="xsd:string"/>
		<xsd:attribute name="Quantity" type="xsd:string"/>
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="ContactDetailsMsgPortuaireNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="LastName" type="OneToFiftyCharType"/>
		<xsd:attribute name="FirstName" type="OneToFiftyCharType"/>
		<xsd:attribute name="Location" type="LocodeType"/>
		<xsd:attribute name="Phone" type="OneToTwentyCharType" use="required"/>
		<xsd:attribute name="Fax" type="OneToTwentyCharType"/>
		<xsd:attribute name="Email" type="OneToFiftyCharType"/>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
							Définition des types de Messages Portuaires
							*******************************************
		</xsd:documentation>
	</xsd:annotation>
	
	<xsd:complexType name="NCA_MsgPortuaireBodyType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="VesselIdentification" type="VesselIdentificationMsgPortuaireNCAType"/>
			<xsd:element name="ShipCallInformation" type="ShipCallInformationMsgPortuaireNCAType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="VesselIdentificationMsgPortuaireNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="IMONumber" type="IMONumberType" use="required"/>
		<xsd:attribute name="MMSINumber" type="MMSINumberType"/>
	</xsd:complexType>
	<xsd:complexType name="ShipCallInformationMsgPortuaireNCAType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="ShipCallId" type="ShipCallIdType" use="required"/>
		<xsd:attribute name="PortOfCall" type="LocodeType" use="required"/>
	</xsd:complexType>	
	<xsd:complexType name="TotalQuantityType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="Quantity" type="DecimalTwoFractionDigitType" use="required"/>
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string" use="required"/>
		<xsd:attribute name="Details" type="ZeroToFiftyCharType"/>
	</xsd:complexType>
	<xsd:complexType name="QuantityGrossNCAFAL7MDType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence />
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string" use="required"/>
		<xsd:attribute name="GrossQuantity" type="DecimalThreeFractionDigitType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="QuantityNetNCAFAL7MDType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence />
		<xsd:attribute name="UnitOfMeasurement" type="xsd:string" use="required"/>
		<xsd:attribute name="NetQuantity" type="DecimalThreeFractionDigitType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="QuantityNCAWasteType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence />
		<xsd:attribute name="Quantity" type="OneToEighteenCharType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ShippingContactDetailsNCAFALType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence />
		<xsd:attribute name="NameOfAgent" type="OneToFiftyCharType"/>
		<xsd:attribute name="Phone" type="OneToTwentyCharType"/>
		<xsd:attribute name="Fax" type="OneToTwentyCharType"/>
		<xsd:attribute name="Email" type="ZeroToFiftyCharType"/>
	</xsd:complexType>
		
	<xsd:complexType name="NCA_72hType">
		<xsd:sequence>
			<xsd:element name="Header" type="NCA_HeaderType"/>
			<xsd:element name="Body" type="NCA_72hBodyType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NCA_72hBodyType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="NCA_MsgPortuaireBodyType">
			<xsd:sequence>
				<xsd:element name="VoyageInformation" type="VoyageInformationNCA72hType"/>
				<xsd:element name="NotificationDetails" type="NotificationDetailsNCA72hType"/>
			</xsd:sequence>	
		</xsd:extension>	
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VoyageInformationNCA72hType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="LastPort" type="LocodeType"/>
		<xsd:attribute name="ETDFromLastPort" type="zsdateTime"/>
		<xsd:attribute name="ETAToPortOfCall" type="zsdateTime" use="required"/>
		<xsd:attribute name="ETDFromPortOfCall" type="zsdateTime" use="required"/>
		<xsd:attribute name="PossibleAnchorage" type="YesNoType"/>		
	</xsd:complexType>
	<xsd:complexType name="NotificationDetailsNCA72hType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="PlannedOperations" type="ZeroToTwoHundredFiftySixCharType"/>
		<xsd:attribute name="PlannedWorks" type="ZeroToTwoHundredFiftySixCharType"/>
		<xsd:attribute name="ShipConfiguration" type="xsd:string"/>
		<xsd:attribute name="CargoVolumeNature" type="ZeroToTwoHundredFiftySixCharType"/>
		<xsd:attribute name="ConditionCargoBallastTanks" type="xsd:string"/>
		<xsd:attribute name="DateOfLastInspection" type="zsdate"/>
	</xsd:complexType>
	
	<xsd:complexType name="NCA_PortType">
		<xsd:sequence>
			<xsd:element name="Header" type="NCA_HeaderType"/>
			<xsd:element name="Body" type="NCA_PortBodyType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NCA_PortBodyType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="NCA_MsgPortuaireBodyType">
				<xsd:sequence>
					<xsd:element name="VoyageInformation" type="VoyageInformationNCAPortType"/>
					<xsd:element name="OtherInformations" type="OtherInformationsNCAPortType" minOccurs="0"/>
				</xsd:sequence>	
			</xsd:extension>	
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VoyageInformationNCAPortType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="LastPort" type="LocodeType"/>
		<xsd:attribute name="ETDFromLastPort" type="zsdateTime"/>
		<xsd:attribute name="ETAToPortOfCall" type="zsdateTime"/>
		<xsd:attribute name="ETDFromPortOfCall" type="zsdateTime"/>
		<xsd:attribute name="TotalPersonsOnBoard" type="OneToFiveIntType" use="required"/>
		<xsd:attribute name="PossibleAnchorage" type="YesNoType"/>
		<xsd:attribute name="PositionInPortOfCall" type="ZeroToFiftyCharType"/>
	</xsd:complexType>
	<xsd:complexType name="OtherInformationsNCAPortType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TotalQuantity" type="TotalQuantityType"  minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="Draught" type="OneToThreeIntType"/>
	</xsd:complexType>
	
	<xsd:complexType name="NCA_ArrivalType">
		<xsd:sequence>
			<xsd:element name="Header" type="NCA_HeaderType"/>
			<xsd:element name="Body" type="NCA_ArrivalBodyType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NCA_ArrivalBodyType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="NCA_MsgPortuaireBodyType">
				<xsd:sequence>
					<xsd:element name="VoyageInformation" type="VoyageInformationNCAArrivalType"/>
				</xsd:sequence>
			</xsd:extension>	
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VoyageInformationNCAArrivalType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence/>
		<xsd:attribute name="OffShore" type="YesNoType"/>
