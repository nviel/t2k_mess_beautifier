	<xsd:element name="NCA_Sitrep" type="NCA_SitrepType"/>
	<xsd:element name="NCA_SitRepCirc" type="NCA_SitrepCircType"/>
	<xsd:element name="NCA_SitRepSar" type="NCA_SitrepSarType"/>
