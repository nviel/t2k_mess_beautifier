	<xsd:element name="NCA_Ano_Portuaire" type="NCA_AnoPortuaireType"/>
	<xsd:element name="NCA_Arrival" type="NCA_ArrivalType"/>
	<xsd:element name="NCA_Departure" type="NCA_DepartureType"/>
	<xsd:element name="NCA_DMS" type="NCA_DMSType"/>
	<xsd:element name="NCA_Event" type="NCA_EventType"/>
	<xsd:element name="NCA_FAL1" type="NCA_FAL1Type"/>
	<xsd:element name="NCA_FAL5" type="NCA_FAL5Type"/>
	<xsd:element name="NCA_FAL6" type="NCA_FAL6Type"/>
	<xsd:element name="NCA_FAL7MD" type="NCA_FAL7MDType"/>
	<xsd:element name="NCA_Form_Res" type="NCA_Form_ResType"/>
	<xsd:element name="NCA_Infraction" type="NCA_InfractionType"/>
	<xsd:element name="NCA_ISPS" type="NCA_ISPSType"/>
	<xsd:element name="NCA_ListeVoyage_Res" type="NCA_ListeVoyage_ResType"/>
	<xsd:element name="NCA_Mouil_Rade" type="NCA_MouilRadeType"/>
	<xsd:element name="NCA_Mouv_Por" type="NCA_MouvPorType"/>
	<xsd:element name="NCA_NavEvtRep" type="NCA_NavEvtRepType"/>
	<xsd:element name="NCA_Navire_Banni" type="NCA_NavireBanniType"/>
	<xsd:element name="NCA_Note" type="NCA_NoteType"/>
	<xsd:element name="NCA_OpCirc" type="NCA_OpCircType"/>
	<xsd:element name="NCA_OpPol" type="NCA_OpPolType"/>
	<xsd:element name="NCA_OpSar" type="NCA_OpSarType"/>
	<xsd:element name="NCA_Ping" type="NCA_PingType"/>
	<xsd:element name="NCA_Polrep" type="NCA_PolrepType"/>
	<xsd:element name="NCA_Port" type="NCA_PortType"/>
	<xsd:element name="NCA_Receipt" type="NCA_ReceiptType"/>
	<xsd:element name="NCA_Ship" type="NCA_ShipType"/>
	<xsd:element name="NCA_ShipAIS" type="NCA_ShipAISType"/>
	<xsd:element name="NCA_ShipID" type="NCA_ShipIDCardType"/>
	<xsd:element name="NCA_ShipID_Req" type="NCA_ShipIDCardReqType"/>
	<xsd:element name="NCA_ShipID_Res" type="NCA_ShipIDCardResType"/>
